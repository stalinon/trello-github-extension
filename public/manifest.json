{
  "name": "Trello to Github",
  "description": "Trello to Github Integration Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://github.com/*", "https://github.com/*"],
      "js": ["./static/js/content.js"]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
